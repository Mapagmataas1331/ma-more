{
	"name": "ma-more",
	"version": "1.0.0",
	"description": "Additional features of Mapagmataas' websites",
	"author": "Mapagmataas",
	"homepage": "https://more.ma.cyou",
	"repository": "github:mapagmataas1331/ma-more",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"postbuild": "npx svelte-sitemap --domain https://more.ma.cyou --out-dir build",
		"deploy": "gh-pages -d build -t true",
		"preview": "vite preview",
		"preview:gh": "http-server build -p 80",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"validate": "npm run check && prettier --write . && eslint ."
	},
	"devDependencies": {
		"@internationalized/date": "^3.6.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.10.1",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.65",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.5.1",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.0-next.1",
		"gh-pages": "^6.2.0",
		"globals": "^15.13.0",
		"lucide-svelte": "^0.466.0",
		"mode-watcher": "^0.5.0",
		"paneforge": "^1.0.0-next.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"sass-embedded": "^1.82.0",
		"svelte": "^5.6.2",
		"svelte-check": "^4.1.1",
		"svelte-radix": "^2.0.1",
		"svelte-sitemap": "^2.6.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.21.1",
		"tailwind-merge": "^2.5.5",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.16",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.17.0",
		"vaul-svelte": "^1.0.0-next.3",
		"vite": "^6.0.2",
		"zod": "^3.23.8"
	}
}
